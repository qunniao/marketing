page{
  background: #F2F1F1;
}

.header{
  width: 100%;
  height: 120rpx;
  background: #fff;
  display: flex;
  align-items: center;
  margin-top: 15rpx;
  position: relative;
}

.yongjinPrice{
  font-size: 32rpx;
  color: #333;
  padding-left: 20rpx;
}

.youbiande{
  position: absolute;
  right: 20rpx;
  display: flex;
  align-items: center;
}

.time{
  font-size: 30rpx;
  color: #666;
  padding-right: 10rpx;
}

.youbiande image{
  width: 25rpx;
  height: 25rpx;
}

.yongjinList{
  margin-top: 15rpx;
}

.yongjinItem{
  width: 100%;
  height: 140rpx;
  background: #fff;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #E5E5E5;
}

.yongjinItem:last-child{
  border: 0;
}

.yongjinItem image{
  width: 80rpx;
  height: 80rpx;
  margin-left: 20rpx;
}

.xiaofei_time{
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}

.xiaofei{
  font-size: 32rpx;
  color: #333;
}

.xfTime{
  font-size: 26rpx;
  color: #888585;
  padding-top: 15rpx;
}

.xfPrice{
  font-size: 32rpx;
  color: #333;
  position: absolute;
  right: 20rpx;
}

.orderList{
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top: 30rpx;
}

.orderItem{
  width: 95%;
  margin-left: 2.5%;
  border-radius: 15rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}

.orderItem:last-child{
  margin-bottom: 20rpx;
}

.orderBianhao_type{
  width: 100%;
  height: 70rpx;
  display: flex;
  position: relative;
  align-items: center;
  margin-top: 10rpx;
}

.bianhao{
  font-size: 28rpx;
  color: #666666;
  padding-left: 20rpx;
}

.changeToCloudStore{
  font-size: 28rpx;
  color: #006eff;
  padding-left: 20rpx;
}

.type{
  font-size: 30rpx;
  color: #fb312f;
  position: absolute;
  right: 20rpx;
}

.img_title_price_num{
  width: 100%;
  display: flex;
  margin-top: 20rpx;
  position: relative;
}

.img{
  width: 25%;
  height: 180rpx;
  border-radius: 10rpx;
  margin-left: 20rpx;
}

.title{
  width: 50%;
  padding-left: 20rpx;
  font-size: 30rpx;
  color: #333;
}

.price_num{
  display: flex;
  flex-direction: column;
  position: absolute;
  right: 20rpx;
  text-align: right;
}

.price{
  font-size: 28rpx;
  color: #333;
}

.num{
  font-size: 30rpx;
  color: #333;
}

.gong_heji{
  width: 100%;
  height: 70rpx;
  position: relative;
  display: flex;
  right: 0;
  text-align: right;
}

.gongde{
  position: absolute;
  right: 20rpx;
}

.gong{
  font-size: 26rpx;
  color: #666666;
}

.heji{
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
  padding-left: 30rpx;
}